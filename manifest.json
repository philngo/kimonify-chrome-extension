{
  "manifest_version": 2,

  "name": "Kimonify",
  "description": "Kimonify a page to make a custom API.",
  "version": "1.0",
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Kimonify"
  },
  "background": {
    "scripts": ["background.js"]
  }
}
